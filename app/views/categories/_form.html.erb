<%= render 'partials/shared', obj: @category %>
<div class='col col-md-6 col-md-offset-3'>
  <%= form_for @category do |f| %>
			<div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control' %>
			</div>
			<%= f.submit  class: 'btn btn-success btn-lg'%>
	<% end %>
</div>